{
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/vst/**",
                "${workspaceFolder}/vst/JUCE/modules",
                "${workspaceFolder}/vst/build/_deps/**"
            ],
            "defines": [
                "JUCE_VST3_CAN_REPLACE_VST2=0"
            ],
            "macFrameworkPath": [
                "/System/Library/Frameworks",
                "/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "compileCommands": "${workspaceFolder}/vst/build/compile_commands.json"
        }
    ],
    "version": 4
}
